{
    "name": "rrbrr/mnews",
    "keywords": ["thinkphp", "news"],
    "description": "An authorization library that supports access control models like ACL, RBAC, ABAC in ThinkPHP 6. ",
    "authors": [
        {
            "name": "caoygx",
            "email": "caoygx@qq.com"
        }
    ],
    "license": "Apache-2.0",
    "require": {
    	"topthink/framework": "6.0.x",
        "topthink/think-migration": "^3.0"
    },
    "require-dev": {
        "topthink/think": "6.0.x"
    },
    "autoload": {
        "psr-4": {
            "mnews\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
          "mnews\\tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "think": {
            "services": [
                "mnews\\NewsService"
            ],
            "config": {
                "mnews": "config/config.php"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
