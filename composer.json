{
    "name": "rrbrr/mpayment",
    "keywords": ["thinkphp", "pay"],
    "description": "",
    "authors": [
        {
            "name": "caoygx",
            "email": "caoygx@qq.com"
        }
    ],
    "license": "Apache-2.0",
    "require": {
    	"topthink/framework": "6.0.x",
        "topthink/think-migration": "^3.0"
    },
    "require-dev": {
        "topthink/think": "6.0.x"
    },
    "autoload": {
        "psr-4": {
            "mpayment\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
          "mpayment\\tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "think": {
            "services": [
                "mpayment\\NewsService"
            ],
            "config": {
                "mpayment": "config/config.php"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
